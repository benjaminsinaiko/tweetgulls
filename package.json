{
  "name": "tweetgulls",
  "version": "1.0.0",
  "description": "Twitter client for staying connected with Brighton and Hove Albion FC Team and Player news",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node ./backend/server.js",
    "server": "nodemon ./backend/server.js --ignore client",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benjaminsinaiko/tweetgulls.git"
  },
