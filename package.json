  "bugs": {
    "url": "https://github.com/benjaminsinaiko/tweetgulls/issues"
  },
  "homepage": "https://github.com/benjaminsinaiko/tweetgulls#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "socket.io": "^2.1.1",
    "twitter": "^1.7.1"
  },
  "devDependencies": {
    "nodemon": "^1.18.6"
  }
}
